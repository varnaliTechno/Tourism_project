<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Tourism.tourism_app">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_logo_s"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_logo_c"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.Tourism.tourism_app.Add_Pack_Item"></activity>
        <activity android:name="com.Tourism.tourism_app.Add_Package" />
        <activity android:name="com.Tourism.tourism_app.Add_Category_Act" />
        <activity android:name="com.Tourism.tourism_app.PackageDetailAct" />
        <activity android:name="com.Tourism.tourism_app.PackageAct" />
        <activity android:name="com.Tourism.tourism_app.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

